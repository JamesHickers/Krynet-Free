{
  "krynet_api": {
    "base_url": "https://krynet.ai/api/",
    "authentication": {
      "required": true,
      "method": "Bearer Token",
      "mfa_required": true,
      "mfa_methods": ["TOTP", "SMS", "Email"]
    },
    "endpoints": {
      "accounts": {
        "login": {
          "method": "POST",
          "path": "/accounts/login"
        },
        "get_profile": {
          "method": "GET",
          "path": "/accounts/me"
        }
      }
    }
  }
}
